{
  "multipart": [
    {   "when": { "up": "true" },
      "apply": { "model": "earthworks:wall_chalk_post" }
    },
    {   "when": { "north": "true" },
      "apply": { "model": "earthworks:wall_chalk_side", "uvlock": true }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "earthworks:wall_chalk_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "true" },
      "apply": { "model": "earthworks:wall_chalk_side", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "earthworks:wall_chalk_side", "y": 270, "uvlock": true }
    }
  ]
}